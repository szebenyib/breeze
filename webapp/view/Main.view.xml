<mvc:View
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:sem="sap.m.semantic"
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
	controllerName="breeze.controller.Main" displayBlock="true">
	<Page>
		<SplitContainer>
			<masterPages>
				<Page
					id="masterPage"
					title="{i18n>MainTitle}">
					<headerContent>
					</headerContent>
					<content>
						<List
							change="onSelectionChange"
							items="{
								path: 'ACDevicesModel>/'
							}">
							<StandardListItem
								description="{ACDevicesModel>id}"
								title="{ACDevicesModel>location}" />
						</List>
					</content>
				</Page>
			</masterPages>
			<detailPages>
				<Page
					title="{i18n>DetailPageTitle}" />
			</detailPages>
		</SplitContainer>
	</Page>
</mvc:View>
